{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "pg": "Host=localhost;Port=5432;Database=avanews;Username=postgres;Password=postgres"
  },
  "Ingestion": {
    "Enabled": true,
    "IntervalMinutes": 60,
    "PageLimit": 100,
    "MaxPagesPerRun": 10,
    "LookbackOverlapMinutes": 1,
    "InitialLookbackMinutes": 180
  },
  "Jwt": {
    "Authority": "https://your-idp-authority",
    "Audience": "news.api"
  },
  "Polygon": {
    "BaseUrl": "https://api.polygon.io/",
    "ApiKey": "your-api-key"
  }
}
